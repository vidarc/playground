{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.3.1",
  "type": "module",
  "scripts": {
    "build": "yarn build:typecheck && yarn build:client && yarn build:server",
    "build:typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "8.4.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/middie": "8.3.1",
    "@fastify/rate-limit": "9.1.0",
    "@fastify/static": "7.0.4",
    "@linaria/atomic": "6.2.0",
    "@linaria/core": "6.2.0",
    "fastify": "4.28.0",
    "history": "5.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.24.0",
    "sanitize.css": "13.0.0",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.2",
    "@tsconfig/cypress": "1.0.2",
    "@tsconfig/esm": "1.0.5",
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/vite-react": "3.0.2",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "@vitejs/plugin-react": "4.3.1",
    "@wyw-in-js/vite": "0.5.3",
    "cypress": "13.12.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "rollup": "4.18.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.2",
    "vite": "5.3.2"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
