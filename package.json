{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.6.4",
  "type": "module",
  "scripts": {
    "build": "yarn build:typecheck && yarn build:client && yarn build:server",
    "build:typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "8.1.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/middie": "8.3.0",
    "@fastify/rate-limit": "8.0.3",
    "@fastify/static": "6.11.2",
    "@linaria/atomic": "5.0.2",
    "@linaria/core": "5.0.2",
    "fastify": "4.23.2",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.16.0",
    "sanitize.css": "13.0.0",
    "swr": "2.2.4"
  },
  "devDependencies": {
    "@linaria/vite": "5.0.3",
    "@testing-library/cypress": "10.0.1",
    "@tsconfig/cypress": "1.0.1",
    "@tsconfig/esm": "1.0.4",
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@tsconfig/vite-react": "2.0.1",
    "@types/node": "20.7.2",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitejs/plugin-react": "4.1.0",
    "cypress": "13.3.0",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "rollup": "3.29.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.9"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
