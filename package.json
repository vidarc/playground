{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.3.1",
  "type": "module",
  "scripts": {
    "build": "yarn build:typecheck && yarn build:client && yarn build:server",
    "build:typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@fastify/helmet": "10.1.0",
    "@fastify/middie": "8.1.0",
    "@fastify/rate-limit": "7.6.0",
    "@fastify/static": "6.7.0",
    "@linaria/atomic": "4.2.5",
    "@linaria/core": "4.2.5",
    "fastify": "4.12.0",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.0",
    "sanitize.css": "13.0.0",
    "swr": "2.0.3",
    "undici": "5.19.1"
  },
  "devDependencies": {
    "@linaria/vite": "4.2.5",
    "@testing-library/cypress": "9.0.0",
    "@tsconfig/cypress": "1.0.1",
    "@tsconfig/node18-strictest-esm": "1.0.1",
    "@tsconfig/vite-react": "1.0.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vitejs/plugin-react": "3.0.1",
    "cypress": "12.4.1",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "rollup": "3.12.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vite": "4.0.4"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
