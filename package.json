{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@compiled/react": "0.10.1",
    "faker": "5.5.3",
    "fastify": "3.25.3",
    "fastify-static": "4.5.0",
    "history": "5.2.0",
    "middie": "5.4.0",
    "react": "18.0.0-rc.0-next-f2a59df48-20211208",
    "react-dom": "18.0.0-rc.0-next-f2a59df48-20211208",
    "react-router-dom": "6.2.1",
    "sanitize.css": "13.0.0",
    "swr": "1.1.2",
    "undici": "4.12.1"
  },
  "devDependencies": {
    "@compiled/babel-plugin": "0.11.3",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/react": "12.1.2",
    "@tsconfig/cypress": "1.0.0",
    "@tsconfig/node16": "1.0.2",
    "@tsconfig/vite-react": "1.0.0",
    "@types/faker": "5.5.9",
    "@types/node": "16.11.17",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "@vitejs/plugin-react": "1.1.3",
    "cypress": "9.2.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "happy-dom": "2.25.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1",
    "rollup": "2.62.0",
    "rollup-plugin-gzip": "3.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "vite": "2.7.10",
    "vitest": "0.0.139"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
