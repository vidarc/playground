{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.10.3",
  "type": "module",
  "scripts": {
    "build": "yarn build:typecheck && yarn build:client && yarn build:server",
    "build:typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint .",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "9.9.0",
    "@fastify/helmet": "13.0.2",
    "@fastify/middie": "9.0.3",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "8.2.0",
    "@linaria/atomic": "6.3.0",
    "@linaria/core": "6.3.0",
    "fastify": "5.6.1",
    "history": "5.3.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.3",
    "sanitize.css": "13.0.0",
    "swr": "2.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@testing-library/cypress": "10.1.0",
    "@tsconfig/cypress": "1.0.3",
    "@tsconfig/esm": "1.0.5",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.6",
    "@tsconfig/vite-react": "6.3.6",
    "@types/node": "22.18.6",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitejs/plugin-react": "4.7.0",
    "@wyw-in-js/vite": "0.7.0",
    "cypress": "14.5.4",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-cypress": "5.1.1",
    "eslint-plugin-import-x": "4.16.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "rollup": "4.52.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
