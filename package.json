{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@compiled/react": "0.10.4",
    "@faker-js/faker": "7.2.0",
    "@fastify/helmet": "8.1.0",
    "@fastify/rate-limit": "6.0.1",
    "@fastify/static": "5.0.2",
    "fastify": "3.29.0",
    "history": "5.3.0",
    "middie": "6.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "sanitize.css": "13.0.0",
    "swr": "1.3.0",
    "undici": "5.5.1"
  },
  "devDependencies": {
    "@compiled/babel-plugin": "0.15.0",
    "@compiled/eslint-plugin": "0.4.1",
    "@testing-library/cypress": "8.0.3",
    "@tsconfig/cypress": "1.0.0",
    "@tsconfig/node16": "1.0.2",
    "@tsconfig/vite-react": "1.0.0",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-react": "1.3.2",
    "cypress": "10.0.3",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.0",
    "prettier": "2.6.2",
    "rollup": "2.75.5",
    "rollup-plugin-gzip": "3.0.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.3",
    "vite": "2.9.10"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
