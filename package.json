{
  "name": "playground",
  "version": "1.0.0",
  "repository": "https://github.com/vidarc/playground",
  "author": "Matthew Ailes <mattailes@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "type": "module",
  "scripts": {
    "build": "yarn build:typecheck && yarn build:client && yarn build:server",
    "build:typecheck": "tsc --noEmit",
    "build:client": "vite build",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir ../dist/ssr && tsc --project tsconfig.server.json",
    "lint": "eslint .",
    "test": "echo 'no tests yet'",
    "start": "ts-node --project tsconfig.server.json src/server/server.ts",
    "start:prod": "node dist/server/server.js",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "9.8.0",
    "@fastify/helmet": "13.0.1",
    "@fastify/middie": "9.0.3",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "8.2.0",
    "@linaria/atomic": "6.3.0",
    "@linaria/core": "6.3.0",
    "fastify": "5.4.0",
    "history": "5.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.3",
    "sanitize.css": "13.0.0",
    "swr": "2.3.3"
  },
  "devDependencies": {
    "@eslint/js": "9.30.0",
    "@testing-library/cypress": "10.0.3",
    "@tsconfig/cypress": "1.0.3",
    "@tsconfig/esm": "1.0.5",
    "@tsconfig/node20": "20.1.6",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/vite-react": "6.3.6",
    "@types/node": "22.15.34",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "@vitejs/plugin-react": "4.6.0",
    "@wyw-in-js/vite": "0.7.0",
    "cypress": "14.5.0",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-cypress": "5.1.0",
    "eslint-plugin-import-x": "4.16.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "rollup": "4.44.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "6.3.5"
  },
  "browserslist": "last 2 Chrome versions",
  "prettier": {
    "singleQuote": true
  }
}
