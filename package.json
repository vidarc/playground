    "fastify-static": "4.5.0",
    "history": "5.3.0",
    "middie": "6.0.0",
    "react": "18.0.0-rc.1-next-f468816ef-20220225",
    "react-dom": "18.0.0-rc.1-next-f468816ef-20220225",
    "react-router-dom": "6.2.2",
    "sanitize.css": "13.0.0",
    "swr": "1.2.2",
    "undici": "4.14.1"
  },
  "devDependencies": {
